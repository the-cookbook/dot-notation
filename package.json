  "scripts": {
    "authors": "node ./scripts/authors.js",
    "prepare": "yarn husky install && npm run build && npm run authors",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags",
    "release": "standard-version",
    "prestart": "npm run clean",
    "start": "yarn webpack serve --mode development --progress --profile",
    "new": "yarn plop",
    "commit": "npx git-cz",
    "build": "npm run clean && yarn tsc && yarn tsc --declaration --emitDeclarationOnly",
    "postbuild": "yarn rollup --config",
    "clean": "rm -rf lib && rm -rf .*cache",
    "test": "yarn jest --no-cache",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn jest --coverage --maxWorkers=2",
    "lint": "yarn run-p lint:*",
    "lint:js": "yarn eslint './src/**/*.{ts,tsx}'",
    "lint:ts": "yarn tsc --noEmit",
    "lint-fix": "yarn run-p lint-fix:*",
    "lint-fix:js": "npm run lint:js --  --fix",
    "prettier": "yarn prettier -c --write 'src/**/*'"
  },
